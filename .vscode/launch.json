{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${webRoot}/*", // for dev app
        "webpack:///./dist/projects/core/*": "${webRoot}/projects/core/*" // for core
      }
    }
  ]
}
